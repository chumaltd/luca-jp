<HOI060 page="1" VR="4.0" id="HOI060" sakuseiNM="<%= @company %>" softNM="<%= @software %> <%= @company %>" sakuseiDay="<%= @issue_date %>">
    <% @有価証券.values.each do |security| %>
        <HFB00000>
            <HFB00110>
                <%= render_attr('HFB00120', security[:security_purpose]) %>
                <%= render_attr('HFB00130', security[:security_genre]) %>
                <%= render_attr('HFB00140', security[:name]) %>
            </HFB00110>
            <HFB00200>
                <%= render_attr('HFB00210', security[:security_units]) %>
                <HFB00215>
                    <%= render_attr('HFB00230', security[:amount]) %>
                </HFB00215>
            </HFB00200>
            <% unless security[:diff].nil? || security[:diff].zero? %>
                <HFB00300>
                    <%= render_attr('HFB00340', security[:diff]) %>
                </HFB00300>
            <% end %>
            <%= render_attr('HFB00400', security[:note]) %>
        </HFB00000>
    <% end %>
    <HFC00000>
        <%= render_attr('HFC00100', 有価証券合計) %>
        <%= render_attr('HFC00200', @有価証券.values.map { |s| s[:diff] }.compact.sum) %>
    </HFC00000>
</HOI060>
