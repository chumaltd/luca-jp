<HOA511 page="1" VR="<%= @form_vers['HOA511'] %>" id="HOA511" sakuseiNM="<%= @company %>" softNM="<%= @software %> <%= @company %>" sakuseiDay="<%= @issue_date %>">
    <ICA00000>
        <ICA00010>
            <ICA00020 IDREF="JIGYO_NENDO_FROM"/>
            <ICA00030 IDREF="JIGYO_NENDO_TO"/>
        </ICA00010>
        <ICA00040 IDREF="NOZEISHA_NM"/>
    </ICA00000>
    <ICB00000>
        <%= 別表五一仮払事業税 %>
        <%= 別表五一還付法人税 %>
        <%= 別表五一還付都道府県住民税 %>
        <%= 別表五一還付市民税 %>
        <ICB00400>
            <%= render_attr('ICB00410', 期首繰越損益) %>
            <ICB00420>
                <%= render_attr('ICB00430', 期首繰越損益) %>
                <%= render_attr('ICB00440', 期末繰越損益) %>
            </ICB00420>
            <%= render_attr('ICB00450', 期末繰越損益) %>
        </ICB00400>
        <ICB00460>
            <%= render_attr('ICB00470', 期首納税充当金) %>
            <ICB00480>
                <%= render_attr('ICB00490', 納税充当金期中減) %>
                <%= render_attr('ICB00500', 当期納税充当金) %>
            </ICB00480>
            <%= render_attr('ICB00510', 当期納税充当金) %>
        </ICB00460>
        <ICB00520>
            <ICB00530>
                <%= render_attr('ICB00540', 期首未納法人税) %>
                <ICB00550>
                    <%= render_attr('ICB00560', @法人税期中減 + @法人税中間納付 + @地方法人税中間納付) %>
                    <ICB00570>
                        <%= render_attr('ICB00580', @法人税中間納付 + @地方法人税中間納付) %>
                        <%= render_attr('ICB00590', 期末未納法人税) %>
                    </ICB00570>
                </ICB00550>
                <%= render_attr('ICB00610', 期末未納法人税) %>
            </ICB00530>
            <%# ICB00860 未払通算税効果額 %>
            <ICB00620>
                <%= render_attr('ICB00630', 期首未納都道府県民税) %>
                <ICB00640>
                    <%= render_attr('ICB00650', @都道府県民税期中減 + @都道府県民税中間納付) %>
                    <ICB00660>
                        <%= render_attr('ICB00670', @都道府県民税中間納付) %>
                        <%= render_attr('ICB00680', 確定都道府県住民税) %>
                    </ICB00660>
                </ICB00640>
                <%= render_attr('ICB00700', 確定都道府県住民税) %>
            </ICB00620>
            <ICB00710>
                <%= render_attr('ICB00720', 期首未納市民税) %>
                <ICB00730>
                    <%= render_attr('ICB00740', @市民税期中減 + @市民税中間納付) %>
                    <ICB00750>
                        <%= render_attr('ICB00760', @市民税中間納付) %>
                        <%= render_attr('ICB00770', (確定市民税 - 市民税損金納付)) %>
                    </ICB00750>
                </ICB00730>
                <%= render_attr('ICB00790', (確定市民税 - 市民税損金納付)) %>
            </ICB00710>
        </ICB00520>
        <ICB00800>
            <%= render_attr('ICB00810', 別表五一期首差引金額) %>
            <ICB00820>
                <%= render_attr('ICB00830', 別表五一期中減差引金額) %>
                <%= render_attr('ICB00840', 別表五一期中増差引金額) %>
            </ICB00820>
            <%= render_attr('ICB00850', 別表五一期末差引金額) %>
        </ICB00800>
    </ICB00000>
    <ICC00000>
        <ICC00010>
            <%= render_attr('ICC00020', @期首資本金) %>
            <ICC00030>
                <%= render_attr('ICC00040', @資本金期中減) %>
                <%= render_attr('ICC00050', @資本金期中増) %>
            </ICC00030>
            <%= render_attr('ICC00060', 期末資本金) %>
        </ICC00010>
        <ICC00070>
            <%= render_attr('ICC00080', @期首資本準備金) %>
            <ICC00090>
                <%= render_attr('ICC00100', @資本準備金期中減) %>
                <%= render_attr('ICC00110', @資本準備金期中増) %>
            </ICC00090>
            <%= render_attr('ICC00120', @期末資本準備金) %>
        </ICC00070>
        <% unless [@期首その他資本剰余金, @その他資本剰余金期中減, @その他資本剰余金期中増, @期末その他資本剰余金].reject { |i| i == 0 }.compact.empty? %>
        <ICC00130>
            <%= render_attr('ICC00140', 'その他資本剰余金') %>
            <%= render_attr('ICC00150', @期首その他資本剰余金) %>
            <ICC00160>
                <%= render_attr('ICC00170', @その他資本剰余金期中減) %>
                <%= render_attr('ICC00180', @その他資本剰余金期中増) %>
            </ICC00160>
            <%= render_attr('ICC00190', @期末その他資本剰余金) %>
        </ICC00130>
        <% end %>
        <% unless [@期首自己株式, @自己株式期中減, @自己株式期中増, @期末自己株式].reject { |i| i == 0 }.compact.empty? %>
            <ICC00130>
                <%= render_attr('ICC00140', '自己株式') %>
                <%= render_attr('ICC00150', @期首自己株式) %>
                <ICC00160>
                    <%= render_attr('ICC00170', @自己株式期中減) %>
                    <%= render_attr('ICC00180', @自己株式期中増) %>
                </ICC00160>
                <%= render_attr('ICC00190', @期末自己株式) %>
            </ICC00130>
        <% end %>
        <ICC00200>
            <%= render_attr('ICC00210', 別表五一期首資本) %>
            <ICC00220>
                <%= render_attr('ICC00230', @資本金等の額期中減) %>
                <%= render_attr('ICC00240', @資本金等の額期中増) %>
            </ICC00220>
            <%= render_attr('ICC00250', 資本金等の額) %>
        </ICC00200>
    </ICC00000>
</HOA511>
