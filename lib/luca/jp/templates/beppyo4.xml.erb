<HOA420 page="1" VR="<%= @form_vers['HOA420'] %>" id="HOA420" sakuseiNM="<%= @company %>" softNM="<%= @software %> <%= @company %>" sakuseiDay="<%= @issue_date %>">
    <ARA00000>
        <ARA00010>
            <ARA00020 IDREF="JIGYO_NENDO_FROM"/>
            <ARA00030 IDREF="JIGYO_NENDO_TO"/>
        </ARA00010>
        <ARA00040 IDREF="NOZEISHA_NM"/>
    </ARA00000>
    <ARB00000>
        <% ['ARB00010', 'ARB00020'].each do |code| %>
            <%= render_attr(code, @当期純損益) %>
        <% end %>
    </ARB00000>
    <ARC00000>
        <ARC00010>
            <% ['ARC00020', 'ARC00030'].each do |code| %>
                <%= render_attr(code, @損金経理をした法人税及び地方法人税) %>
            <% end %>
        </ARC00010>
        <ARC00040>
            <% ['ARC00050', 'ARC00060'].each do |code| %>
                <%= render_attr(code, @損金経理をした道府県民税及び市町村民税) %>
            <% end %>
        </ARC00040>
        <ARC00100>
            <% ['ARC00110', 'ARC00120'].each do |code| %>
                <%= render_attr(code, @損金経理をした納税充当金) %>
            <% end %>
        </ARC00100>
        <% if @減価償却の償却超過額 %>
            <ARC00160>
                <% ['ARC00170', 'ARC00180'].each do |code| %>
                    <%= render_attr(code, @減価償却の償却超過額) %>
                <% end %>
            </ARC00160>
        <% end %>
        <% if @役員給与の損金不算入額 %>
            <ARC00190>
                <% ['ARC00200', 'ARC00210'].each do |code| %>
                    <%= render_attr(code, @役員給与の損金不算入額) %>
                <% end %>
            </ARC00190>
        <% end %>
        <% if @交際費等の損金不算入額 %>
            <ARC00213>
                <% ['ARC00215', 'ARC00218'].each do |code| %>
                    <%= render_attr(code, @交際費等の損金不算入額) %>
                <% end %>
            </ARC00213>
        <% end %>
        <%# ARC00360 通算法人に係る加算額 %>
        <%= 別表四還付事業税 %>
        <ARC00325>
            <% ['ARC00330', 'ARC00340'].each do |code| %>
                <%= render_attr(code, @損金不算入額) %>
            <% end %>
        </ARC00325>
    </ARC00000>
    <ARD00000>
        <% if @減価償却超過額の当期認容額 %>
            <ARD00010>
                <% ['ARD00020', 'ARD00030'].each do |code| %>
                    <%= render_attr(code, @減価償却超過額の当期認容額) %>
                <% end %>
            </ARD00010>
        <% end %>
        <ARD00040>
            <% ['ARD00050', 'ARD00060'].each do |code| %>
                <%= render_attr(code, @納付事業税) %>
            <% end %>
        </ARD00040>
        <% if @受取配当金の益金不算入額 %>
            <ARD00070>
                <% ['ARD00080', 'ARD00090'].each do |code| %>
                    <%= render_attr(code, @受取配当金の益金不算入額) %>
                <% end %>
            </ARD00070>
        <% end %>
        <% if @受贈益の益金不算入額 %>
            <ARD00340>
                <% ['ARD00350', 'ARD00360'].each do |code| %>
                    <%= render_attr(code, @受贈益の益金不算入額) %>
                <% end %>
            </ARD00340>
        <% end %>
        <%# ARD00400 通算法人に係る減算額 %>
        <% if @翌期還付事業税 > 0 %>
            <ARD00215>
                <%= render_attr('ARD00220', '仮払事業税認定損') %>
                <% ['ARD00230', 'ARD00240'].each do |code| %>
                    <%= render_attr(code, @翌期還付事業税) %>
                <% end %>
            </ARD00215>
        <% end %>
        <ARD00280>
            <%= render_attr('ARD00290', @益金不算入額) %>
            <%= render_attr('ARD00300', @益金不算入額留保) %>
            <ARD00310>
                <%= render_attr('ARD00330', @益金不算入額社外流出) %>
            </ARD00310>
        </ARD00280>
    </ARD00000>
    <ARE00000>
        <%= render_attr('ARE00010', 別表四調整所得仮計) %>
        <%= render_attr('ARE00020', 別表四調整所得仮計留保) %>
        <ARE00030>
            <%= render_attr('ARE00050', 別表四調整所得仮計社外流出) %>
        </ARE00030>
    </ARE00000>
    <ARZ10000>
        <%= render_attr('ARZ10010', 別表四調整所得仮計) %>
        <%= render_attr('ARZ10020', 別表四調整所得仮計留保) %>
        <ARZ10030>
            <%= render_attr('ARZ10050', 別表四調整所得仮計社外流出) %>
        </ARZ10030>
    </ARZ10000>
    <ARF00000>
        <%= render_attr('ARF00010', 寄付金の損金不算入額) %>
        <%= render_attr('ARF00020', 寄付金の損金不算入額) %>
    </ARF00000>
    <% if @所得税等の損金不算入額 %>
        <ARI00000>
            <%= render_attr('ARI00010', @所得税等の損金不算入額) %>
            <%= render_attr('ARI00020', @所得税等の損金不算入額) %>
        </ARI00000>
    <% end %>
    <%# ARJ00000 税額控除の対象となる外国法人税の額 %>
    <ARK00000>
        <%= render_attr('ARK00010', 別表四調整所得合計) %>
        <%= render_attr('ARK00020', 別表四調整所得合計留保) %>
        <ARK00030>
            <%= render_attr('ARK00050', 別表四調整所得合計社外流出) %>
        </ARK00030>
    </ARK00000>
    <ARM00000>
        <%= render_attr('ARM00010', 別表四調整所得合計 - @繰越損失管理.deduction) %>
        <%= render_attr('ARM00020', 別表四調整所得合計留保) %>
        <ARM00030>
            <%= render_attr('ARM00050', 別表四調整所得合計社外流出 - @繰越損失管理.deduction) %>
        </ARM00030>
    </ARM00000>
    <%# ARN10010 中間申告における繰戻しによる還付に係る災害損失欠損金額の益金算入額 %>
    <%# ARR00000 非適格合併又は残余財産の全部分配等による移転資産等の譲渡利益額又は譲渡損失額 %>
    <ARS00000>
        <%= render_attr('ARS00010', @別表四調整所得) %>
        <%= render_attr('ARS00020', 別表四調整所得合計留保) %>
        <ARS00030>
            <%= render_attr('ARS00050', 別表四調整所得合計社外流出) %>
        </ARS00030>
    </ARS00000>
    <%# ARS10000 更生欠損金又は民事再生等評価換えが行われる場合の再生等欠損金の損金算入額 %>
    <%# ARS20000 通算対象欠損金額の損金算入額又は通算対象所得金額の益金算入額 %>
    <ARS50000>
        <%= render_attr('ARS50010', @別表四調整所得) %>
        <%= render_attr('ARS50020', 別表四調整所得合計留保) %>
        <ARS50030>
            <%= render_attr('ARS50050', 別表四調整所得合計社外流出) %>
        </ARS50030>
    </ARS50000>
    <ART00000>
        <% ['ART00010', 'ART00020'].each do |code| %>
            <%= render_attr(code, @繰越損失管理.deduction) %>
        <% end %>
    </ART00000>
    <%# ARX00000 残余財産の確定の日の属する事業年度に係る事業税及び特別法人事業税の損金算入額 %>
    <ARV00000>
        <%= render_attr('ARV00010', @別表四調整所得 - @繰越損失管理.deduction) %>
        <%= render_attr('ARV00020', 別表四調整所得合計留保) %>
        <ARV00030>
            <%= render_attr('ARV00050', 別表四調整所得合計社外流出 - @繰越損失管理.deduction) %>
        </ARV00030>
    </ARV00000>
</HOA420>
